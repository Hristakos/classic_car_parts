<h1>My Listings</h1>

<% listings.each do |listing|%>
   <div> <a href="/user/listings/<%= listing["id"]%>"><%= listing["headline"] %></a>
   <img src=<%= listing["img_url"]%> alt="">
   <p>suburb: <%= listing["suburb"]%></p>
   <p> post date: <%= DateTime.parse(listing["post_date"]).ctime%></p>
   <p> price: <%= listing["price"]%></p></div>
<% end%>

